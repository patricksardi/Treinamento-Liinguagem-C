// Digitar numeros inteiros e mostrar os Negativos na Tela.

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n;

    printf("quantos numeros voce vai digitar? ");
    scanf("%d", &n);

    int vet [n];

    for (int i = 0 ; i < n ; i++)
    {
        printf("\n digite um numero:\n");
        scanf("%d", &vet[i]);
    }
    printf("\nnumeros negativos:\n");
    for (int i = 0 ; i < n ; i++)
    {
        if(vet[i] < 0)
        {
            printf("%d\n", vet[i]);
        }

    }

    return 0;
}
